import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
	    LinkedList<Integer> list=new LinkedList<>();
	    for(int i=0;i<n;i++){
	        list.add(sc.nextInt());
	    }
	   System.out.println("Enter node to find");
	   int c=sc.nextInt();
	   System.out.println(list.get(n-c));
	}
}
