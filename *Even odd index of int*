import java.util.*;
public class Main
{
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		String s=Integer.toString(n);
		int e=0;
		int o=0;
		for(int i=0;i<s.length();i++){
		    if(i%2==0){
		        e=e+(s.charAt(i)-'0');
		    }
		    else if(i%2!=0){
		        o=o+(s.charAt(i)-'0');
		    }
		}
		System.out.println(e);
		System.out.println(o);
	}
}
