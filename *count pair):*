import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Integer a[]={0,-1,2,1,3,5};
	    Set<Integer> set = new HashSet<Integer> (Arrays.asList(a)) ; 
	    int t=0;
	    int k=8;
	    for(int i=0;i<a.length;i++){
	            if(set.contains(a[i]-k) || set.contains(k-a[i])){
	                t++;
	    }
	    }
		System.out.println(t/2);
	}
}
