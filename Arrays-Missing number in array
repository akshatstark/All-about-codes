// its actually maths... sum of n no. - no. os arr sum
class Solution {
    int missingNumber(int array[], int n) {
        
        int ans= (n*(n+1))/2;    // sum of n no.
        
        int sum=0;
        for(int i=0;i<n-1;i++){
            sum=sum+array[i];
        }
        return ans-sum;
    }
}
