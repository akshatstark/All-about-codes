import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    String str="abbba";
	    String res="";
	    for(int i=str.length()-1;i>=0;i--){
	        res=res+str.charAt(i);
	        
	    }
	    if(res.equals(str))
		System.out.println("YES");
		else
		System.out.println("NO");
	}
}
