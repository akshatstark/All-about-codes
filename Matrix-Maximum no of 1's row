class Sol
{
    public static int maxOnes (int Mat[][], int N, int M)
    {
        int max=0;
        int res=0;
        for(int i=0;i<N;i++){
            int temp=0;
            for(int j=0;j<M;j++){
                
                if(Mat[i][j]==1) temp++;
            }
            if(temp>max){
                max=temp;
                res=i;
            } 
        }
    return res;
    }
}
