import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
	    int num=n;
	    int c=0;int rem=0;
	    while(n>0){
	        rem=n%10;	       
	        c=c+rem;     	        
	        n=n/10;	      
	    }
		    if(num%c==0)
	        System.out.println("Harshad no.");
	    
	    else 
	        System.out.println("Not a Harshad no.");
	    
	}
}
